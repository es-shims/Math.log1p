{
	"root": true,

	"extends": "@ljharb",

	"rules": {
		"id-length": 0,
		"new-cap": [2, {
			"capIsNewExceptions": [
				"GetIntrinsic",
				"ToNumber"
			],
		}],
		"no-magic-numbers": 0,
	},

	"overrides": [
		{
			"files": "**/*.mjs",
			"rules": {
				"no-restricted-exports": 0,
			},
		},
	],
}
