{
	"name": "math.log1p",
	"version": "1.0.6",
	"description": "An ES-spec-compliant Math.log1p shim/polyfill/replacement that works as far down as ES3",
	"main": "index.js",
	"exports": {
		".": [
			{
				"import": "./index.mjs",
				"require": "./index.js",
				"default": "./index.js"
			},
			"./index.js"
		],
		"./auto": "./auto.js",
		"./polyfill": "./polyfill.js",
		"./implementation": "./implementation.js",
		"./shim": "./shim.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"prepack": "npmignore --auto --commentLines=autogenerated",
		"prepublishOnly": "safe-publish-latest",
		"prepublish": "not-in-publish || npm run prepublishOnly",
		"pretest": "npm run lint",
		"prelint": "es-shim-api --bound",
		"lint": "eslint --ext=js,mjs .",
		"postlint": "evalmd README.md",
		"tests-only": "nyc tape 'test/**/*.js'",
		"tests-esm": "nyc node test/index.mjs",
		"test": "npm run tests-only && npm run tests-esm",
		"posttest": "aud --production",
		"version": "auto-changelog && git add CHANGELOG.md",
		"postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/es-shims/Math.log1p.git"
	},
	"keywords": [
		"Math.log1p",
		"math",
		"log1p",
		"log",
		"shim",
		"polyfill",
		"es-shim",
		"API"
	],
	"author": "Nicol√≤ Ribaudo <nicolo.ribaudo@gmail.com>",
	"funding": {
		"url": "https://github.com/es-shims/Math.log1p?sponsor=1"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/es-shims/Math.log1p/issues"
	},
	"homepage": "https://github.com/es-shims/Math.log1p#readme",
	"devDependencies": {
		"@es-shims/api": "^2.5.0",
		"@ljharb/eslint-config": "^21.1.0",
		"aud": "^2.0.4",
		"auto-changelog": "^2.4.0",
		"eslint": "=8.8.0",
		"evalmd": "^0.0.19",
		"functions-have-names": "^1.2.3",
		"has-strict-mode": "^1.0.1",
		"in-publish": "^2.0.1",
		"npmignore": "^0.3.1",
		"nyc": "^10.3.2",
		"object-is": "^1.1.6",
		"safe-publish-latest": "^2.0.0",
		"tape": "^5.7.5"
	},
	"dependencies": {
		"call-bind": "^1.0.2",
		"define-properties": "^1.2.0",
		"es-abstract": "^1.22.1",
		"get-intrinsic": "^1.2.1"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false,
		"hideCredit": true
	},
	"publishConfig": {
		"ignore": [
			".github/workflows"
		]
	}
}
